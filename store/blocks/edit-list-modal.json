{
  "modal-trigger#edit-list-button": {
    "children": ["rich-text#edit-list-button", "modal-layout#edit-list"],
    "props": {
      "blockClass": ["edit-list"]
    }
  },
  "rich-text#edit-list": {
    "props": {
      "text": "![](assets/add.svg)",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "edit-list"
    }
  },
  "modal-layout#edit-list": {
    "children": ["modal-header#edit-list", "edit-list-form"],
    "props": {
      "blockClass": "edit-list",
      "fullScreen": true
    }
  },
  "modal-header#edit-list": {
    "children": ["rich-text#edit-list-modal-header"],
    "props": {
      "blockClass": ["container", "edit-list"],
      "showCloseButton": true
    }
  },
  "rich-text#edit-list-modal-header": {
    "props": {
      "text": "Editar Informações",
      "textAlignment": "LEFT",
      "blockClass": "modal-header-title"
    }
  },
  "modal-actions.close#edit-list": {
    "props": {
      "label": "Cancelar"
    }
  }
}
