{
  "store.home": {
    "blocks": ["auth-condition#lists","auth-condition#search-list" ,"flex-layout.row#advantages"],
    "parent": { "storeWrapper": "storeWrapper#home" }
  },
  "auth-condition#search-list": {
    "props": {
      "Then": "flex-layout.row#search-list"
    }
  },
  "auth-condition#lists": {
    "props": {
      "Then": "flex-layout.row#lists"
    }
  },
  "flex-layout.row#search-list": {
    "children": ["flex-layout.col#search-list-col"],
    "props": {
      "blockClass": "search-list"
    }
  },
  "flex-layout.col#search-list-col": {
    "children": ["rich-text#home-search-list-title", "search-list"],
    "props": {
      "blockClass": "search-list-col"
    }
  },
  "rich-text#home-search-list-title": {
    "props": {
      "text": "Encontre uma Lista",
      "blockClass": "home-search-list-title"
    }
  }
}
